{
	"name": "@sysvale/cuida",
	"version": "3.0.0-alpha.21",
	"description": "A design system built by Sysvale, using storybook and Vue components",
	"repository": {
		"type": "git",
		"url": "https://github.com/Sysvale/cuida"
	},
	"main": "./dist/@sysvale/cuida.umd.js",
	"module": "./dist/@sysvale/cuida.es.js",
	"files": [
		"dist/*"
	],
	"exports": {
		".": {
			"import": "./dist/@sysvale/cuida.es.js",
			"require": "./dist/@sysvale/cuida.umd.js"
		},
		"./dist/style.css": "./dist/style.css"
	},
	"unpkg": "dist/@sysvale/cuida.umd.js",
	"scripts": {
		"dev": "vite",
		"build": "vue-tsc --noEmit && vite build",
		"preview": "vite preview",
		"storybook": "start-storybook --docs --no-manager-cache",
		"build-storybook": "build-storybook -s public --docs",
		"test": "vitest"
	},
	"dependencies": {
		"@sysvale/cuida-icons": "^1.5.3",
		"@sysvale/vue3-long-click": "^0.2.0",
		"click-outside-vue3": "^4.0.1",
		"lodash.clonedeep": "^4.5.0",
		"lodash.isempty": "^4.4.0",
		"lodash.isequal": "^4.5.0",
		"vue": "^3.2.37",
		"vue-multiselect": "^3.0.0-alpha.2",
		"vue-slider-component": "^4.1.0-beta.7"
	},
	"devDependencies": {
		"@babel/core": "^7.18.10",
		"@rollup/plugin-typescript": "^8.4.0",
		"@storybook/addon-actions": "^6.5.10",
		"@storybook/addon-essentials": "^6.5.10",
		"@storybook/addon-interactions": "^6.5.10",
		"@storybook/addon-links": "^6.5.10",
		"@storybook/builder-vite": "^0.2.2",
		"@storybook/preset-scss": "^1.0.3",
		"@storybook/testing-library": "^0.0.13",
		"@storybook/vue3": "^6.5.10",
		"@vitejs/plugin-vue": "^3.0.3",
		"@vue/test-utils": "^2.0.2",
		"babel-loader": "^8.2.5",
		"concat": "^1.0.3",
		"jsdom": "^20.0.0",
		"sass": "^1.55.0",
		"typescript": "^4.6.4",
		"vite": "^3.0.0",
		"vitest": "^0.22.0",
		"vue-loader": "^16.8.3",
		"vue-tsc": "^0.38.4"
	}
}
